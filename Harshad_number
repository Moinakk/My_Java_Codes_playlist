class Main{
     static int harShadNumber(int n){
        int sum=0;
        int rem=0;
        while(n>0){

            rem=n%10;
            sum=sum+rem;
            n/=10;

        }
        return sum;
    }
    public static void main(String[]args){
        int num=156;
        int sum1=harShadNumber(num);
        if(num%sum1==0){
            System.out.println("Harshad Number");
        }
        else{
            System.out.println("Not harshad");
        }
    }
}
