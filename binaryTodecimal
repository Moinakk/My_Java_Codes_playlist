class Main{
    public static void main(String[]args){
        int num=3;
        int rem;
        int binary=0;
        int i=1;
        while(num!=0){
            rem=num%2;
            binary=binary+rem*i;
            num=num/2;
            i=i*10;
        }
        System.out.println(binary);

    }
}
